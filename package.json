{
  "name": "link-zijin-dev",
  "version": "0.0.1",
  "description": "a link re-directer",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "./node_modules/nodemon/bin/nodemon.js --exec ts-node src/server.ts",
    "start-prod": "node ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "jest"
  },
  "author": "Zijin Luo",
  "license": "Apache-2.0",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|tsx|js|jsx)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.20",
    "typegoose": "^5.6.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.3.24",
    "@types/node": "^10.14.4",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  }
}
